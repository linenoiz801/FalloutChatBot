@model IEnumerable<FalloutChat.Models.ChatHistoryListItem>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MessageSent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SentTimeUtc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResponseReceived)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReceviedTimeUtc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BadResponse)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MessageSent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SentTimeUtc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResponseReceived)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReceviedTimeUtc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BadResponse)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
